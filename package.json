{
  "name": "ts-aio",
  "version": "0.2.0-alpha.2",
  "description": "ts test & build & dev all in one package",
  "main": "dist/index.js",
  "bin": {
    "ts-aio": "dist/index.js"
  },
  "types": "dist/index.d.ts",
  "repository": "https://github.com/bichikim/ts-allinone.git",
  "author": "BichiKim <bichi@live.co.kr>",
  "license": "MIT",
  "scripts": {
    "build": "ts-node src",
    "lint": "concurrently \"npm:lint-eslint\" \"npm:lint-tslint\"",
    "lint-eslint": "eslint --ext .js --ext .ts src",
    "lint-tslint": "tslint src/**/*.ts",
    "reformat": "ts-node src reformat",
    "_help": "ts-node src --help",
    "test": "ts-node src test",
    "test-watch": "ts-node src test-watch",
    "preversion": "npm run reformat && npm run test && npm run build",
    "postversion": "npm publish",
    "coverage": "ts-node src coverage"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "commander": "^2.19.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "del": "^4.0.0",
    "eslint": "^5.15.1",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.16.0",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "eslint-plugin-vue": "^5.2.2",
    "fs-extra": "^8.0.0",
    "gulp": "^4.0.0",
    "gulp-shell": "^0.7.0",
    "gulp-typescript": "^5.0.1",
    "lodash": "^4.17.11",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.11",
    "through2": "^3.0.1",
    "ts-node": "^8.0.3",
    "tsconfig": "^7.0.0",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.14.0",
    "typescript": "^3.3.3333"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/fs-extra": "^5.0.5",
    "@types/graphql": "^14.0.7",
    "@types/lodash": "^4.14.123",
    "@types/mocha": "^5.2.6",
    "@types/module-alias": "^2.0.0",
    "@types/mongoose": "^5.3.22",
    "@types/node": "^11.11.3",
    "@types/sinon": "^7.0.10",
    "chai": "^4.2.0"
  }
}
